<p id="notice"><%= notice %></p>

<h1>Invoices</h1>

<table>
  <thead>
    <tr>
      <th>Cnpj</th>
      <th>Social name</th>
      <th>Fantasy name</th>
      <th>Public place</th>
      <th>District</th>
      <th>County</th>
      <th>Uf</th>
      <th>Country</th>
      <th>Cpf</th>
      <th>Name addressee</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @invoices.each do |invoice| %>
      <tr>
        <td><%= invoice.cnpj %></td>
        <td><%= invoice.social_name %></td>
        <td><%= invoice.fantasy_name %></td>
        <td><%= invoice.public_place %></td>
        <td><%= invoice.district %></td>
        <td><%= invoice.county %></td>
        <td><%= invoice.uf %></td>
        <td><%= invoice.country %></td>
        <td><%= invoice.cpf %></td>
        <td><%= invoice.name_addressee %></td>
        <td><%= link_to 'Show', invoice %></td>
        <td><%= link_to 'Edit', edit_invoice_path(invoice) %></td>
        <td><%= link_to 'Destroy', invoice, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Invoice', new_invoice_path %>
